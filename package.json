{
  "name": "sveltekit-local-storage",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --port 3030",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "prepare": "husky install",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "svelte-check": "svelte-check --fail-on-hints"
  },
  "devDependencies": {
    "@fontsource/source-sans-pro": "^4.5.0",
    "@fontsource/source-serif-pro": "^4.5.0",
    "@sveltejs/adapter-static": "^1.0.0-next.21",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vanilla-extract/css": "^1.6.3",
    "@vanilla-extract/css-utils": "^0.1.1",
    "@vanilla-extract/vite-plugin": "3.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "husky": "^7.0.4",
    "prettier": "~2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sharp": "0.28.2",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.9.8",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "vanilla-lazyload": "^17.5.0",
    "vite-imagetools": "^3.8.0"
  },
  "type": "module"
}
